{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/pages/*": ["pages/*"],
      "@/styles/*": ["styles/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types.d.ts",
    ".next/types/**/*.ts",
    "app/Components/Header/Header.jsx",
    "app/user/layout.jsx",
    "app/Components/Buttons/DarkModeButton.jsx",
    "app/(admin1)/[[...index]]/head.tsx",
    "app/(admin1)/[[...index]]/loading.tsx",
    "app/(admin1)/[[...index]]/page.tsx",
    "app/Components/sanity/StudioNavbar.jsx",
    "app/Components/sanity/RichTextComponents.jsx",
    "app/Components/sanity/PreviewSuspense.jsx",
    "app/Components/sanity/PreviewBlogList.jsx",
    "app/Components/sanity/Logo.jsx",
    "app/Components/sanity/Header.jsx",
    "app/Components/sanity/ClientSideRoute.jsx",
    "app/Components/sanity/BlogList.jsx",
    "app/Components/sanity/Banner.jsx",
    "utils/sanity/urlFor.js",
    "utils/sanity/sanity.preview.js",
    "utils/sanity/sanity.client.js",
    "theme.js",
    "pages/api/preview.js"
  ],
  "exclude": ["node_modules"]
}
